{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["react", "prettier", "jsx-a11y", "import"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": "off",
    "import/newline-after-import": 0,
    "class-methods-use-this": 0,
    "no-console": "off",
    "import/order": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "no-redeclare": 0,
    "no-useless-constructor": 0,
    "no-empty-function": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "no-unused-vars": 0,
    "import/no-mutable-exports": 0,
    "no-plusplus": 0,
    "no-else-return": 0,
    "no-case-declarations": 0,
    "no-unused-expressions": 0,
    "no-sequences": 0,
    "no-return-assign": 0,
    "no-self-assign": 0,
    "no-useless-return": 0,
    "no-lonely-if": 0,
    "camelcase": 0,
    "prefer-destructuring": 0,
    "no-restricted-syntax": 0,
    "no-fallthrough": 0
  }
}
